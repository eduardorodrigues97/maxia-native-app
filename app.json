{
  "expo": {
    "name": "Max.ia Professor",
    "slug": "maxia-professor",
    "version": "1.0.7",
    "owner": "maxia",
    "orientation": "portrait",
    "icon": "./assets/maxia-logo-playstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen-maxia.png",
      "backgroundColor": "#f2f2f2",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "education.maxia.professor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/maxia-logo-playstore.png",
        "backgroundColor": "#f2f2f2"
      },
      "package": "education.maxia.professor",
      "versionCode": 7,
      "googleServicesFile": "./auth/google-services.json"
    },
    "web": {
      "favicon": "./assets/maxia-logo-playstore.png"
    },
    "description": "",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/maxia-logo-playstore.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
